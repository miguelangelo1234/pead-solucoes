{
  "manifest_version": 3,
  "name": "Fix ES Modules Extension",
  "version": "1.0",
  "description": "Demonstrates fixing ES Modules error in Chrome extensions",
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content-loader.js"]
    }
  ],
  "permissions": []
}
